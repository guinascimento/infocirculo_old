pdf.text full_name, :size => 23;pdf.text @profile.user.professional_description + " - " + @profile.user.industry.name;pdf.text @profile.user.email;pdf.text "\n";pdf.text "Informacoes Pessoais", :size => 14, :style => :bold;pdf.text "Telefone: " + @profile.phone;pdf.text "Endereco: " + @profile.address;pdf.text "IM: " + full_im;pdf.text "Estado Civil: " + @profile.maritial_status.to_s;pdf.text "\n";pdf.text "Resumo", :size => 14, :style => :bold;pdf.text @profile.summary;pdf.text "\n";pdf.text "Experiencia Profissional", :size => 14, :style => :bold;@profile.experiences.each do |experience|	pdf.text experience.title + " - " + experience.company_name, :size => 12, :style => :bold;	pdf.text experience.start + " - " + experience.end;	pdf.text experience.description;endpdf.text "\n";;